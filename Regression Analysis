   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "duration\n",
      "q3: 18.383333333333333\n",
      "upper_threshold: 88.68333333333331\n",
      "count    22685.000000\n",
      "mean        14.464108\n",
      "std         11.934861\n",
      "min          0.000000\n",
      "25%          6.666667\n",
      "50%         11.183333\n",
